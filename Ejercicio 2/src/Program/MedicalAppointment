public class MedicalAppointment
{
    public string Name { get; set; }
    public string Id { get; set; }
    public string PhoneNumber { get; set; }
    public DateTime Date { get; set; }
    public string AppointmentPlace { get; set; }
    public string DoctorName { get; set; }
    public string AppointmentId { get; set; } // Identificador Ãºnico de la cita

    public MedicalAppointment(string name, string id, string phoneNumber, DateTime date, string appointmentPlace, string doctorName, string appointmentId)
    {
        Name = name;
        Id = id;
        PhoneNumber = phoneNumber;
        Date = date;
        AppointmentPlace = appointmentPlace;
        DoctorName = doctorName;
        AppointmentId = appointmentId;
    }
}
